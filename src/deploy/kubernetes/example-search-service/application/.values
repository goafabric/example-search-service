_Release_namespace=example
_Values_root_name=example-search-service
_Values_app_name=${_Values_root_name}-application
_Values_ingress_name=search

_Values_image_repository=goafabric/example-search-service
_Values_image_tag=3.1.0-SNAPSHOT

_Values_service_type=ClusterIP
_Values_service_port=8080
_Values_service_nodePort_dev=30950

_Values_resources_requests_memory=256M
_Values_resources_requests_cpu=250m
_Values_readinessProbe_initialDelaySeconds=10

_Values_command=org.goafabric.example.searchservice.Application